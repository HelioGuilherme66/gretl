Windows Registry Editor Version 5.00

[HKEY_LOCAL_MACHINE\Software\gretl]
"gretldir"="@GHOME@"

; gretl data files (.gdt)

[HKEY_CLASSES_ROOT\Software\gretl\.gdt]
@="GretlDataFile"
"Content Type"="application/x-gretldata"

[HKEY_CLASSES_ROOT\Software\gretl\GretlDataFile]
@="Gretl data file"

[HKEY_CLASSES_ROOT\Software\gretl\GretlDataFile\DefaultIcon]
@="@GEXEPATH@\\gretl.exe,1"

[HKEY_CLASSES_ROOT\Software\gretl\GretlDataFile\shell\open\command]
@="\"@GEXEPATH@\\gretlrun.cmd\" \"%1\""

; gretl script files (.inp)

[HKEY_CLASSES_ROOT\Software\gretl\.inp]
@="GretlScriptFile"
"Content Type"="application/x-gretlscript"

[HKEY_CLASSES_ROOT\Software\gretl\GretlScriptFile]
@="Gretl script file"

[HKEY_CLASSES_ROOT\Software\gretl\GretlScriptFile\DefaultIcon]
@="@GEXEPATH@\\gretl.exe,2"

[HKEY_CLASSES_ROOT\Software\gretl\GretlScriptFile\shell\open\command]
@="\"@GEXEPATH@\\gretlrun.cmd\" \"%1\""

; gretl session files (.gretl)

[HKEY_CLASSES_ROOT\Software\gretl\.gretl]
@="GretlSessionFile"
"Content Type"="application/x-gretlsession"

[HKEY_CLASSES_ROOT\Software\gretl\GretlSessionFile]
@="Gretl session file"

[HKEY_CLASSES_ROOT\Software\gretl\GretlSessionFile\DefaultIcon]
@="@GEXEPATH@\\gretl.exe,3"

[HKEY_CLASSES_ROOT\Software\gretl\GretlSessionFile\shell\open\command]
@="\"@GEXEPATH@\\gretlrun.cmd\" \"%1\""

; gretl binary data files (.gdtb)

[HKEY_CLASSES_ROOT\Software\gretl\.gdtb]
@="GretlBindataFile"
"Content Type"="application/x-gretlbindata"

[HKEY_CLASSES_ROOT\Software\gretl\GretlBindataFile]
@="Gretl binary data file"

[HKEY_CLASSES_ROOT\Software\gretl\GretlBindataFile\DefaultIcon]
@="@GEXEPATH@\\gretl.exe,4"

[HKEY_CLASSES_ROOT\Software\gretl\GretlBindataFile\shell\open\command]
@="\"@GEXEPATH@\\gretlrun.cmd\" \"%1\""
