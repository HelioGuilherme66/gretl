topsrc = ..

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
CC = @CC@
CFLAGS = @CFLAGS@
MAKE = @MAKE@
INSTALL = @INSTALL@
LN = @LN_S@
have_readline = @have_readline@
have_zlib = @have_zlib@
need_libbind = @need_libbind@
have_gtk = @have_gtk@
have_gtkextra = @have_gtkextra@
have_gnome = @have_gnome@
gnome_prefix = @gnome_prefix@

GNOME_CFLAGS = @GNOME_CFLAGS@
GNOME_LIBS = @GNOME_LIBS@
GTK_CFLAGS = @GTK_CFLAGS@
GTK_LIBS = @GTK_LIBS@
GTK_EXTRA_CFLAGS = @GTK_EXTRA_CFLAGS@
GTK_EXTRA_LIBS = @GTK_EXTRA_LIBS@
PIXBUF_CFLAGS = @PIXBUF_CFLAGS@
PIXBUF_LIBS = @PIXBUF_LIBS@
USE_NLS = @USE_NLS@

ifeq ($(CC),)
  CC = gcc
endif
ifeq ($(INSTALL_PROGRAM),)
  INSTALL_PROGRAM = $(INSTALL) -m 755
endif
ifeq ($(INSTALL_DATA),)
  INSTALL_DATA = $(INSTALL) -m 644
endif
ifeq ($(LN),)
  LN = ln -sf
endif
ifeq ($(need_libbind),yes)
  INETLIB = -lbind
endif
ifeq ($(gnome_prefix),)
  gnome_prefix = `gnome-config --prefix`
endif
ifeq ($(have_gnome),yes)
  LIBS = $(GNOME_LIBS) $(GTK_EXTRA_LIBS) ../lib/libgretl.la
  GUI_CFLAGS = $(GNOME_CFLAGS) $(GTK_EXTRA_CFLAGS)
  desktopdir = $(gnome_prefix)/share/gnome/apps/Applications
  mimedir = $(gnome_prefix)/share/mime-info
  gpixmaps = $(gnome_prefix)/share/pixmaps
  ghelpdir = $(gnome_prefix)/share/gnome/help/gretl
  query_install_gnome = install-gnome
else
  LIBS = $(GTK_LIBS) $(GTK_EXTRA_LIBS) ../lib/libgretl.la
  GUI_CFLAGS = $(GTK_CFLAGS) $(GTK_EXTRA_CFLAGS)
endif
ifeq ($(USE_NLS),yes)
  datadir = @datadir@
  localedir = $(datadir)/locale
  DEFS = -DLOCALEDIR=\"$(localedir)\"
endif

override CFLAGS += -I$(libsrc) $(GUI_CFLAGS) $(PIXBUF_CFLAGS) $(DEFS)
override LIBS += $(PIXBUF_LIBS)

# Directories
bindir	= $(prefix)/bin
gretldir = $(prefix)/share/gretl
tooldir = $(topsrc)/tools
libsrc = $(topsrc)/lib/src
clisrc = $(topsrc)/cli
docdir = $(topsrc)/doc

#### End of system configuration section. ####

SHELL = /bin/sh
LIBTOOL = $(topsrc)/libtool
PROG  = gretl_x11

vpath %.c $(topsrc)/cli

OBJS = gretl.o library.o gui_utils.o callbacks.o dialogs.o ssheet.o \
	datafiles.o database.o fileselect.o gpt_control.o session.o \
	webget.o calculator.o htmlprint.o guiprint.o boxplots.o \
	selector.o

$(PROG): $(OBJS)
	$(LIBTOOL) --mode=link $(CC) -o $@ $^ $(LIBS) $(INETLIB)

.PHONY:

install: $(PROG) install-data $(query_install_gnome)
	$(LIBTOOL) $(INSTALL_PROGRAM) $(PROG) $(bindir)/$(PROG)

install-strip: $(PROG) install-data $(query_install_gnome)
	$(LIBTOOL) $(INSTALL_PROGRAM) -s $(PROG) $(bindir)/$(PROG)

install-data: installdirs
	$(INSTALL_PROGRAM) $(topsrc)/gretl_sh $(bindir)/gretl
	$(INSTALL_DATA) gretl-logo.xpm $(gretldir)
	$(INSTALL_DATA) $(topsrc)/COPYING $(gretldir)

install-exec: $(PROG) installdirs 
	$(LIBTOOL) $(INSTALL_PROGRAM) $(PROG) $(bindir)/$(PROG)

install-gnome: $(PROG) installdirs 
	$(tooldir)/mkinstalldirs $(gpixmaps)
	$(INSTALL_DATA) gnome/gretl-gnome-logo.xpm $(gpixmaps)
	$(INSTALL_DATA) gnome/gretl.png $(gpixmaps)
	$(INSTALL_DATA) gnome/gnome-*.png $(gpixmaps)
	$(tooldir)/mkinstalldirs $(desktopdir)
	$(INSTALL_DATA) gnome/gretl.desktop $(desktopdir)
	$(tooldir)/mkinstalldirs $(mimedir)
	$(INSTALL_DATA) gnome/gretl.mime $(mimedir)
	$(INSTALL_DATA) gnome/gretl.keys $(mimedir)
	$(tooldir)/mkinstalldirs $(ghelpdir)
	$(tooldir)/mkinstalldirs $(ghelpdir)/C
	$(tooldir)/mkinstalldirs $(ghelpdir)/C/figures
	$(INSTALL_DATA) gnome/topic.dat $(ghelpdir)/C
	$(INSTALL_DATA) $(docdir)/manual-html/*.html $(ghelpdir)/C
	$(INSTALL_DATA) $(docdir)/manual-html/*.css $(ghelpdir)/C
	$(INSTALL_DATA) $(docdir)/figures/*.png $(ghelpdir)/C/figures

installdirs:
	$(tooldir)/mkinstalldirs $(bindir) 
	$(tooldir)/mkinstalldirs $(gretldir)

clean:
	rm -f *.o $(PROG) core
	rm -rf .libs

depend:
	makedepend -Y$(libsrc) *.c 

distdepend: depend
	$(tooldir)/dist_depend.pl

