  <definitions>
  
    <define-regex id="R">R|r</define-regex>
    <define-regex id="octave">Octave|octave</define-regex>
    <define-regex id="ox">Ox|ox</define-regex>
    <define-regex id="stata">Stata|stata|STATA</define-regex>
    <define-regex id="python">Python|python</define-regex>
  
    <context id="foreign-R" style-inside="true">
      <start>^\s*(foreign)\s+language=\%{R}(.*)</start>
      <end>^\s*end\s+foreign</end>
      <include>
        <context sub-pattern="1" where="start" style-ref="command"/>
	<context sub-pattern="2" where="start" style-ref="option"/>
	<context sub-pattern="0" where="end" style-ref="command"/>
        <context ref="r:r"/>
      </include>
    </context>
    
    <context id="foreign-stata" style-inside="true">
      <start>^\s*(foreign)\s+language=\%{stata}(.*)</start>
      <end>^\s*end\s+foreign</end>
      <include>
        <context sub-pattern="1" where="start" style-ref="command"/>
	<context sub-pattern="2" where="start" style-ref="option"/>
	<context sub-pattern="0" where="end" style-ref="command"/>
        <context ref="stata:stata"/>
      </include>
    </context>
    
    <context id="foreign-python" style-inside="true">
      <start>^\s*(foreign)\s+language=\%{python}(.*)</start>
      <end>^\s*end\s+foreign</end>
      <include>
        <context sub-pattern="1" where="start" style-ref="command"/>
	<context sub-pattern="2" where="start" style-ref="option"/>
	<context sub-pattern="0" where="end" style-ref="command"/>
        <context ref="python:python"/>
      </include>
    </context>
    
    <context id="foreign-ox" style-inside="true">
      <start>^\s*(foreign)\s+language=\%{ox}(.*)</start>
      <end>^\s*end\s+foreign</end>
      <include>
        <context sub-pattern="1" where="start" style-ref="command"/>
	<context sub-pattern="2" where="start" style-ref="option"/>
	<context sub-pattern="0" where="end" style-ref="command"/>
        <context ref="cpp:cpp"/>
      </include>
    </context>
    
    <context id="foreign-octave" style-inside="true">
      <start>^\s*(foreign)\s+language=\%{octave}(.*)</start>
      <end>^\s*end\s+foreign</end>
      <include>
        <context sub-pattern="1" where="start" style-ref="command"/>
	<context sub-pattern="2" where="start" style-ref="option"/>
	<context sub-pattern="0" where="end" style-ref="command"/>
        <context ref="gretl-octave"/>
      </include>
    </context>
    
    <context id="block-comment" style-ref="comment">
      <start>/\*</start>
      <end>\*/</end>
      <include>
        <context ref="def:escape"/>
        <context ref="def:line-continue"/>
      </include>
    </context>

