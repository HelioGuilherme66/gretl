open kmenta.gdt
Kmenta <- system
equation Q 0 P D
equation Q 0 P F A
endog Q P
end system
estimate Kmenta method=ols
estimate Kmenta method=tsls
estimate Kmenta method=3sls
restrict Kmenta
  b[1,3] - b[2,4] = 0
  -b[1,2] + b[2,2] = 0.5
end restrict
estimate Kmenta method=ols
estimate Kmenta method=tsls
estimate Kmenta method=3sls
