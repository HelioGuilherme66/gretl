nulldata 200

set normal_rand box-muller

# generate x and y with ties
set seed 321321321
genr x = int(100 * uniform())
genr y = int(100 * uniform())
corr x y
corr x y --spearman
corr x y --kendall

# generate x and y (probably) without ties
set seed 321321321
genr x = 10 + normal())
genr y = 10 + normal())
corr x y
corr x y --spearman
corr x y --kendall

