set verbose off

string PkgName = "fdensity"
pkg query @PkgName --quiet
bundle Pkg = $result
if !nelem(Pkg)
    pkg install @PkgName
    include @PkgName.gfn
endif


open mroz87.gdt --quiet
gpbuild Plots
   freq WA --silent
   fdensity(WA, LFP)
end gpbuild
gridplot Plots --output=display


print "Successfully finished tests."
quit
