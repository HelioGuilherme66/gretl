#: gretl_functions_en.xml:3022(para)
msgid "<placeholder-1/><sub>2</sub> = <placeholder-2/>"
msgstr ""

#: gretl_functions_en.xml:3026(para)
msgid "The three alternatives above provide, generally, a trade-off between accuracy and speed. You can choose among methods by using the <cmdref targ=\"set\"/> command and specify the value 0, 1 or 2 for the <lit>fdjac_quality</lit> variable."
msgstr ""

#: gretl_functions_en.xml:3032(para)
