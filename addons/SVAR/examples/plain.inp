set verbose off
include SVAR.gfn
open sw_ch14.gdt

series infl = 400*ldiff(PUNEW)
rename LHUR unemp
scalar order = 3

list X = unemp infl
list Z = const

Mod = SVAR_setup("plain", X, Z, order)
Mod.horizon = 40
SVAR_estimate(&Mod)

fevdmat = FEVD(&Mod)
print fevdmat

var order X --quiet --variance
set stopwatch
bfail = SVAR_boot(&Mod) # old: ..., 1024, 0.90)
printf "Number of failed = %d, Time (bootstrap) = %g\n", bfail, $stopwatch

series shock1 = GetShock(&Mod, 1)
series shock2 = GetShock(&Mod, 2)
