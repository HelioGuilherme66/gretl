(* William Greene, Econometric Analysis, Example 12.1 *)
open greene12_1.gdt
genr inc2 = income*income
(* exclude obs where expenditure is zero *)
smpl -r expend > 0
ols expend 0 age ownrent income inc2 -o
(* gretl's "hccm" procedure is more conservative than White *)
hccm expend 0 age ownrent income inc2 -o

