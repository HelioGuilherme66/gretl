(* PS11.3, using DATA11-1, for Example 11.3 on exponential smoothing *)
genr time
genr Y8 = TOTALEMP
(* generate lamda X sub t for lambda = 0.2 *)
genr X2 = TOTALEMP*0.2
(* Because the model is autoregressive, sim command should be used *)
help sim
sim 1983.02 1986.12 Y8 X2 0.8
genr Y3 = TOTALEMP
(* generate lamda X sub t for lambda = 0.7 *)
genr X7 = TOTALEMP*0.7
sim 1983.02 1986.12 Y3 X7 0.3
print -o TOTALEMP Y8 Y3
