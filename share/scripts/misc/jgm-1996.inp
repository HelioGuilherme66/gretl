# Replicate part of Table 1 in MacKinnon, JAE 1996
open jgm-data.gdt
smpl 1955 ;
# unit root tests for short-term interest rate
adf 0 r_s --c
adf 0 r_s --ctt
adf 1 r_s --ctt
# unit root tests for medium-term interest rate
adf 0 r_m --c
adf 0 r_m --ctt --verbose
adf 1 r_m --ctt --verbose
# tests for long rate
adf 0 r_l --ctt
adf 1 r_l --ctt
# CPI inflation
adf 1 pi_c --ctt
# inflation by GDP deflator
adf 1 pi_y --ctt
adf 2 pi_y --ctt
