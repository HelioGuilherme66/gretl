#ifndef CONFIG_H
#define CONFIG_H

/* for NLS use */
#define PACKAGE gretl

/* Native language support? */
#undef ENABLE_NLS

/* Location of shared gretl files */
#define GRETL "/usr/local/share/gretl"

/* Define if you want GNU readline support */
#undef HAVE_READLINE

/* Define for GNU readline version >= 4.2 */
#undef NEW_READLINE

/* Define if zlib is available */
#undef HAVE_ZLIB

/* Define if gtkhtml is available */
#undef USE_GTKHTML

/* Is gnuplot available? */
#undef HAVE_GNUPLOT

/* Does gnuplot support PNG output? */
#undef GNUPLOT_PNG

/* Is LaTeX available? */
#undef HAVE_LATEX

/* Should we use Gnome? */
#undef USE_GNOME

/* Does unistd.h have getdomainname? */
#undef GETDOMAINNAME

/* Do we have paths.h? */
#undef HAVE_PATHS_H

#endif /* CONFIG_H */
