#ifndef CONFIG_H
#define CONFIG_H

/* Using GTK version 2.0? */
#undef USE_GTK2

/* for NLS use */
#define PACKAGE gretl

/* Native language support? */
#undef ENABLE_NLS

/* Location of shared gretl files */
#define GRETL_PREFIX "/usr/local"

/* Multiple precision arithmetic? */
#undef ENABLE_GMP

/* Define if you want GNU readline support */
#undef HAVE_READLINE

/* Define for GNU readline version >= 4.2 */
#undef NEW_READLINE

/* Define if zlib is available */
#undef HAVE_ZLIB

/* Define if gtkhtml is available */
#undef USE_GTKHTML

/* Is gnuplot available? */
#undef HAVE_GNUPLOT

/* Does gnuplot support PNG output? */
#undef GNUPLOT_PNG

/* Is LaTeX available? */
#undef HAVE_LATEX

/* Should we use Gnome? */
#undef USE_GNOME

/* Does unistd.h have getdomainname? */
#undef GETDOMAINNAME

/* Do we have unistd.h */
#undef HAVE_UNISTD_H

/* Do we have dirent.h */
#undef HAVE_DIRENT_H

/* Define if the `long double' type works.  */
#undef HAVE_LONG_DOUBLE

/* Define as the return type of signal handlers (int or void).  */
#undef RETSIGTYPE

/* Define if you have the ANSI C header files.  */
#undef STDC_HEADERS

/* Define if your processor stores words with the most significant
   byte first (like Motorola and SPARC, unlike Intel and VAX).  */
#undef WORDS_BIGENDIAN

/* The number of bytes in a int.  */
#undef SIZEOF_INT

/* Define if you have the <string.h> header file.  */
#undef HAVE_STRING_H

#endif /* CONFIG_H */
